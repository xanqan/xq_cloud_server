        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xanqan.project.mapper.PermissionMapper">

    <resultMap id="BaseResultMap" type="com.xanqan.project.model.domain.Permission">
            <id property="id" column="id" jdbcType="INTEGER"/>
